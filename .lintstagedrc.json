{
  "packages/ui/**/*.{js,jsx,ts,tsx}": [
    "pnpm --filter @ani/ui lint"
  ],
  "packages/config/**/*.{js,jsx,ts,tsx}": [
    "pnpm --filter @ani/config lint"
  ],
  "apps/web/**/*.{js,jsx,ts,tsx}": [
    "pnpm --filter web lint"
  ],
  "**/*.{css,scss}": [
    "stylelint --fix",
    "prettier --write"
  ]
}